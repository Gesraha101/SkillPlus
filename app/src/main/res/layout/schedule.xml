<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:tools="http://schemas.android.com/tools"
              android:layout_width="match_parent"
              android:layout_height="wrap_content">

    <android.support.v7.widget.CardView
            xmlns:card_view="http://schemas.android.com/apk/res-auto"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="16dp"
            card_view:cardCornerRadius="8dp"
            card_view:cardElevation="8dp"
            card_view:cardBackgroundColor="@color/colorCardBackground">

            <android.support.constraint.ConstraintLayout
                    android:id="@+id/skill_related"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    card_view:layout_constraintStart_toStartOf="parent"
                    card_view:layout_constraintTop_toBottomOf="@+id/poster_rate">

                <TextView
                        android:id="@+id/schadual"
                        android:layout_width="0dp"
                        android:layout_height="19dp"
                        android:text="@string/schadual_time"
                        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
                        android:textSize="18sp"
                        android:textStyle="bold"
                        card_view:layout_constraintBottom_toBottomOf="parent"
                        card_view:layout_constraintTop_toTopOf="parent" tools:ignore="MissingConstraints"/>

                <Button
                        android:id="@+id/add_to_my_schadual"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:background="@drawable/btn_add"
                        card_view:layout_constraintEnd_toEndOf="parent" tools:ignore="MissingConstraints"/>

            </android.support.constraint.ConstraintLayout>

    </android.support.v7.widget.CardView>

</LinearLayout>

